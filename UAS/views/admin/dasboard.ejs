
<%- include('../partials/head.ejs') %>
<link rel="stylesheet" href="/css/dashboard.css">
<link rel="stylesheet" href="/css/products.css">
</head>
<body>
    <%- include('../partials/dashboard-menu.ejs') %>

    <main role="main" class="col-md-9 ml-sm-auto col-lg-10 px-4">
        <div class="col-sm-12">
            <h2>Dashboard</h2>
        </div>
        <div class="row">
            <div class="col-12 bottom-pad-30" style="margin-bottom: 10px;">
                <div class="card">
                    <div class="card-body text-center">
                        <h5 class="card-title">Products for sale</h5>
                        <h6 class="card-subtitle mb-2 text-muted">Number of products for sale</h6>
                        <h4 class="card-text text-danger">
                            <%= dashboardData.productsCount %>
                        </h4>
                    </div>
                </div>
            </div>
        </div>
    </main>