    <style>
        .title h1,
        .title p {
            text-align: center;
        }

        .title h1 {
            font-size: 48px;
            margin: 40px 0 10px;
            text-decoration: underline;
        }

        .title p {
            margin-bottom: 40px;
        }

        .description {
            background-color: lightblue;
            margin: 50px 15px;
            padding: 100px 60px;
            text-align: justify;
        }

        h2 {
            margin-left: 15px;
        }

        .profile {
            background-color: lightgray;
            margin: 5px 15px 50px;
            padding: 60px 40px;
        }

        .profile .col-md {
            margin: 0 20px;
        }

        .profile img {
            height: 350px;
            object-fit: cover;
            margin-bottom: 20px;
        }

        .profile h3,
        .profile p {
            width: 100%;
            margin: 3px;
        }

        .profile ul {
            list-style: none;
            margin-top: 20px;
            margin-bottom: 0;
        }

        .profile li {
            display: inline;
            margin-right: 20px;
        }

        .profile ul ion-icon {
            color: #5a5a5a;
        }

        #github:hover {
            color: black;
            transition: color 0.2s;
        }

        /* animation */
        .description,
        .profile {
            opacity: 0;
            transition-duration: 1s;
        }

        .description.animated,
        .profile.animated {
            opacity: 1;
        }
    </style>
</head>

